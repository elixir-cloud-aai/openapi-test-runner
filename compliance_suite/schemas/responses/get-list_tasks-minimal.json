{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "$ref": "#/definitions/GetListTasksMinimal",
  "definitions": {
    "GetListTasksMinimal": {
      "type": "object",
      "properties": {
        "tasks": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "state": {
                  "type": "string",
                  "enum": [
                    "UNKNOWN",
                    "QUEUED",
                    "INITIALIZING",
                    "RUNNING",
                    "PAUSED",
                    "COMPLETE",
                    "EXECUTOR_ERROR",
                    "SYSTEM_ERROR",
                    "CANCELED"
                  ]
                }
              }
            }
          ]
        }
      },
      "required": [
        "tasks"
      ]
    }
  }
}